<h1>Your Selected Artist</h1>

<hr>

<a href="/profile">Back to Profile</a>

<hr>

<% artists.forEach(function(artist) { %>
    <p>Artist: <%= artist.displayName %></p>
    <p><%= artist.id %></p>
    <p>On Tour Until: <%= artist.onTourUntil %></p>
    <form action="/artists" method="POST">
        <input name="id" value="<%= artist.id %>" type="hidden">
        <input name="displayName" value="<%= artist.displayName %>" type="hidden">
        <input name="onTourUntil" value="<%= artist.onTourUntil %>" type="hidden">
        <input type="submit" value="save!">
    </form>
    <hr>
<% }) %>

